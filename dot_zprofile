# .zprofile

# Get the aliases and functions
if [ -f ~/.zshrc ]; then
	. ~/.zshrc
fi

# User specific environment and startup programs
PATH=$PATH:$HOME/.local/bin:$HOME/bin:$HOME/Library/Python/2.7/bin:$HOME/Library/Python/3.7/bin:$HOME/.cargo/bin:$HOME/arcanist/bin:$HOME/Library/Python/3.8/bin
export PATH
